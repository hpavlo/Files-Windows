    <!--ScrollViewerStyles-->

    <SolidColorBrush x:Key="ThumbBackgroundColor" Color="#DFDFDF"/>

    <!--FileListViewStyles-->

    <SolidColorBrush x:Key="RepeatButtonBackgroundColor" Color="Transparent"/>
    <SolidColorBrush x:Key="RepeatButtonMouseBackgroundColor" Color="#DFDFDF"/>
    <SolidColorBrush x:Key="RepeatButtonPathFillColor" Color="#8A8A8A"/>

    <SolidColorBrush x:Key="ColumnHeaderGripperFillColor" Color="Transparent"/>
    <SolidColorBrush x:Key="ColumnHeaderForegroundColor" Color="Black"/>
