    <SolidColorBrush x:Key="ComboBoxToggleButtonBorderBrushColor" Color="LightGray"/>
    <SolidColorBrush x:Key="ComboBoxToggleButtonArrowColor" Color="Gray"/>

    <!--ContextMenuStyle-->

    <SolidColorBrush x:Key="SeparatorColor" Color="LightGray"/>
    <SolidColorBrush x:Key="CheckBorderColor" Color="Gray"/>
    <SolidColorBrush x:Key="CheckMarkColor" Color="Gray"/>
    <SolidColorBrush x:Key="TriangleColor" Color="Gray"/>

    <SolidColorBrush x:Key="HeaderForegroundColor" Color="Black"/>
    <SolidColorBrush x:Key="InputGestureTextForegroundColor" Color="Gray"/>

    <SolidColorBrush x:Key="IsHighlightedBackgroundColor" Color="#EAEAEA"/>
    <SolidColorBrush x:Key="MenuBackgroundColor" Color="#FAFAFA"/>
    <SolidColorBrush x:Key="MenuBorderBrushColor" Color="LightGray"/>

