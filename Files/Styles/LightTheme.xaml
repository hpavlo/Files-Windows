    <SolidColorBrush x:Key="TitleMouseCloseButtonBackgroundColor" Color="#C42B1C"/>
    <SolidColorBrush x:Key="TitleNotActiveForegroundColor" Color="#999999"/>

    <SolidColorBrush x:Key="AlwaysOnTopIndicatorColor" Color="#999999"/>

    <!--ButtonStyles-->

    <SolidColorBrush x:Key="ButtonForegroundColor" Color="Black"/>
    <SolidColorBrush x:Key="ButtonBackgroundColor" Color="Transparent"/>
    <SolidColorBrush x:Key="ButtonMouseBackgroundColor" Color="#88E5E5E5"/>
    <SolidColorBrush x:Key="TabButtonMouseBackgroundColor" Color="#88E5E5E5"/>

    <SolidColorBrush x:Key="TreeViewButtonForegroundColor" Color="Black"/>
    <SolidColorBrush x:Key="InstrumentButtonForegroundColor" Color="Black"/>

    <SolidColorBrush x:Key="PathListButtonBackgroundColor" Color="#FAFAFA"/>

    <SolidColorBrush x:Key="PopupButtonForegroundColor" Color="Black"/>
    <SolidColorBrush x:Key="PopupButtonBackgroundColor" Color="#F0F0F0"/>
