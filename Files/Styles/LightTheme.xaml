    <SolidColorBrush x:Key="ToolTipForegroundColor" Color="DarkSlateGray"/>
    <SolidColorBrush x:Key="ToolTipBackgroundColor" Color="#F3F3F3"/>
    <SolidColorBrush x:Key="ToolTipBorderBrushColor" Color="#CACACA"/>

