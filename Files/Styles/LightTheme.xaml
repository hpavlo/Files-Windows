    <Color x:Key="ItemBackgroundColor">#EEFFFFFF</Color>
    <SolidColorBrush x:Key="ItemSelectedBackgroundColor" Color="#AAEAEAFF"/>
    <SolidColorBrush x:Key="ItemBorderBrushColor" Color="White"/>
    <SolidColorBrush x:Key="ItemMouseBorderBrushColor" Color="#CACACA"/>

    <SolidColorBrush x:Key="LoadingIndicatorStrokeColor" Color="#AAAAAA"/>

    <SolidColorBrush x:Key="FooterBackgroundColor" Color="White"/>
    <SolidColorBrush x:Key="FooterSplitterFillColor" Color="#CACACA"/>
    <SolidColorBrush x:Key="ProgressBarTrackBorderBrushColor" Color="LightGray"/>
    <SolidColorBrush x:Key="ProgressBarIndicatorBackgroundColor" Color="LightBlue"/>

    <!--PopupControl-->

    <SolidColorBrush x:Key="PopupBackgroundColor" Color="#FAFAFA"/>
    <SolidColorBrush x:Key="PopupBorderBrushColor" Color="LightGray"/>

    <!--Popups-->

    <SolidColorBrush x:Key="PopupForegroundColor" Color="Black"/>

