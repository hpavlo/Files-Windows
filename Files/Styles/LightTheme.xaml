    <SolidColorBrush x:Key="ToggleButtonColor" Color="#9A9A9A"/>

    <!--ComboBoxStyle-->

    <SolidColorBrush x:Key="ComboBoxItemMouseBackgroundColor" Color="#EAEAEA"/>
