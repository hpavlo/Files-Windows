    <SolidColorBrush x:Key="TextBoxSelectionBrushColor" Color="#CACACA"/>
    <SolidColorBrush x:Key="TextBoxBorderBrushColor" Color="#CACACA"/>
    <SolidColorBrush x:Key="TextBoxCaretBrushColor" Color="#CACACA"/>
    
