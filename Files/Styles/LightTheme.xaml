<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ImageSource x:Key="BackgroundImage">pack://siteoforigin:,,,/Resources/Backgrounds/light.png</ImageSource>

    <!--WindowStyle-->

    <SolidColorBrush x:Key="WindowBackgroundColor" Color="White"/>
    <SolidColorBrush x:Key="TitleBarForegroundColor" Color="Black"/>
    <SolidColorBrush x:Key="TitleBarBackgroundColor" Color="White"/>

    <SolidColorBrush x:Key="TitleMouseButtonForegroundColor" Color="Black"/>
